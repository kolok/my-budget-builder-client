{
  "name": "Pipauls-client",
  "version": "1.0.0",
  "description": "Pipauls frontend client",
  "author": "Nicolas Oudard",
  "license": "private",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "watch": "webpack-dev-server --env.NODE_ENV=development --host 0.0.0.0 --inline --content-base --port=5000",
    "start": "webpack-dev-server --env=dev",
    "build": "rm -rf dist && webpack --env=prod",
    "analyse": "webpack --json > stats.json",
    "eslint": "./node_modules/.bin/eslint src --ext .vue",
    "eslint-fix": "./node_modules/.bin/eslint src --ext .vue --fix"
  },
  "dependencies": {
    "apexcharts": "^3.26.1",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.6.0",
    "d3": "^5.16.0",
    "echarts": "^4.9.0",
    "element-ui": "^2.15.1",
    "es6-promise": "^4.2.8",
    "moment": "^2.29.1",
    "vue": "^2.6.12",
    "vue-i18n": "^8.24.3",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^4.5.2",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
